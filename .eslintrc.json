{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "semi":0,
        "comma-dangle":0,
        "no-param-reassign":0,
        "import/extensions":0,
        "no-console":0,
        "operator-linebreak":0
        
    }
}
